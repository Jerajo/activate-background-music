{
  "atom-workspace": {
    "alt-a": "activate-background-music:toggle",
    "shift-alt-p": "activate-background-music:play/pasue",
    "shift-alt-s": "activate-background-music:stop",
    "shift-alt-r": "activate-background-music:repeat",
    "shift-alt-m": "activate-background-music:muteToggle",
    "alt-right": "activate-background-music:next",
    "alt-left": "activate-background-music:previous",
    "alt-up": "activate-background-music:volumeUp",
    "alt-down": "activate-background-music:volumeDown"
  },
  "atom-text-editor:not([mini])": {
    "alt-right": "activate-background-music:next",
    "alt-left": "activate-background-music:previous"
  }
}
